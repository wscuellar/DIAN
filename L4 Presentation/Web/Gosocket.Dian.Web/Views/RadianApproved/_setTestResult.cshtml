            @Html.Label("Seleccione el modo de operación", new { @class = "control-label" })
            @Html.TextBoxFor(m => m.RadianTestSetResult.OperationModeName, defaultHtmlAttr)
        </div>
    </div>
    <div class="col-sm-3 padding-xs-0">
        <div class="form-group">
            @*@Html.LabelFor(m => m.RadianTestSetResult.Description, new { @class = "control-label" })
            @Html.TextBoxFor(m => m.RadianTestSetResult.Description, defaultHtmlAttr)
            @Html.ValidationMessageFor(m => m.RadianTestSetResult.Description, "", new { @class = "text-danger" })*@
