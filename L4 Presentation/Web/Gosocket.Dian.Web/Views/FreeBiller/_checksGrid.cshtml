
<div>
    <h3 class="panel-title-radian padding-0">Permisos</h3>
    <hr class="margin-0 margin-bottom-10" />
</div>
<div>
    <span class="margin-right-10">Perfiles:</span><ul id="list"></ul>
</div>

<div>
    <div>
        <em class="fa fa-file-text-o fa-5x" aria-hidden="true"></em>
        <h5>Factura Electrónica</h5>
    </div>
    <div class="row checks-group">
        @foreach (var menuMain in Model.MenuOptionsByProfile)
        {
            if (menuMain.MenuLevel == 1 && menuMain.Children.Count > 0 && menuMain.Name == "Facturación")
            {
                <div class="col-sm-3">
                    <div class="level-1">
                        <div>

                        </div>
                        @foreach (var subMenuItems in menuMain.Children)
                        {
                            if (subMenuItems.MenuLevel == 2 && subMenuItems.Name == "Documentos")
                            {
                                <div class="level-2">
                                    <div>
                                        <span class="subTitleMenu">@subMenuItems.Name</span>
                                    </div>
                                    @foreach (var subSubMenuItems in subMenuItems.Children)
                                    {
                                        if (subSubMenuItems.Name == "Factura de venta" || subSubMenuItems.Name == "Factura AIU" || subSubMenuItems.Name == "Factura de exportación"
                                            || subSubMenuItems.Name == "Factura de Mandatorio" || subSubMenuItems.Name == "Factura de Contingencia" || subSubMenuItems.Name == "Nota Crédito"
                                            || subSubMenuItems.Name == "Nota Débito")
                                        {
                                            <div class="level-3">
                                                <div class="row-cols-1">
                                                    <div class="form-group d-flex margin-0">
                                                        <input type="checkbox" id="@subSubMenuItems.Id" name="ValuesSelected" value="@menuMain.Id,@subMenuItems.Id,@subSubMenuItems.Id" disabled />
                                                        <label for="@subSubMenuItems.Id" class="label-check">@subSubMenuItems.Name</label>
                                                    </div>
                                                </div>
                                            </div>
                                        }

                                    }
                                </div>
                            }

                        }
                    </div>
                </div>
            }
            if (menuMain.MenuLevel == 1 && menuMain.Children.Count > 0 && menuMain.Name == "Documentos")
            {
                <div class="col-sm-3">
                    <div class="level-1">
                        <div>

                        </div>
                        @foreach (var subMenuItems in menuMain.Children)
                        {
                            if (subMenuItems.MenuLevel == 2)
                            {
                                <div class="level-2">
                                    <div>
                                        <span class="subTitleMenu">@subMenuItems.Name</span>
                                    </div>
                                    @foreach (var subSubMenuItems in subMenuItems.Children)
                                    {
                                        if (subSubMenuItems.Name == "Documentos emitidos" || subSubMenuItems.Name == "Documentos recibidos")
                                        {
                                            <div class="level-3">
                                                <div class="row-cols-1">
                                                    <div class="form-group d-flex margin-0">
                                                        <input type="checkbox" id="@subSubMenuItems.Id" name="ValuesSelected" value="@menuMain.Id,@subMenuItems.Id,@subSubMenuItems.Id" disabled />
                                                        <label for="@subSubMenuItems.Id" class="label-check">@subSubMenuItems.Name</label>
                                                    </div>
                                                </div>
                                            </div>
                                        }

                                    }
                                </div>
                            }
                        }
                    </div>
                </div>
            }
            if (menuMain.MenuLevel == 1 && menuMain.Children.Count > 0 && menuMain.Name == "Configuración")
            {
                <div class="col-sm-3">
                    <div class="level-1">
                        <div>

                        </div>
                        @foreach (var subMenuItems in menuMain.Children)
                        {
                            if (subMenuItems.MenuLevel == 2 && subMenuItems.Name == "Administrar")
                            {
                                <div class="level-2">
                                    <div>
                                        <span class="subTitleMenu">@subMenuItems.Name</span>
                                    </div>
                                    @foreach (var subSubMenuItems in subMenuItems.Children)
                                    {
                                        if (subSubMenuItems.Name == "Rangos de Numeración" || subSubMenuItems.Name == "Adquiriente/Comprador"
                                            || subSubMenuItems.Name == "Productos" && subSubMenuItems.ParentId == 10)
                                        {
                                            <div class="level-3">
                                                <div class="row-cols-1">
                                                    <div class="form-group d-flex margin-0">
                                                        <input type="checkbox" id="@subSubMenuItems.Id" name="ValuesSelected" value="@menuMain.Id,@subMenuItems.Id,@subSubMenuItems.Id" disabled />
                                                        <label for="@subSubMenuItems.Id" class="label-check">@subSubMenuItems.Name</label>
                                                    </div>
                                                </div>
                                            </div>
                                        }

                                    }
                                </div>
                            }
                        }
                    </div>
                </div>
            }
            if (menuMain.MenuLevel == 1 && menuMain.Children.Count > 0 && menuMain.Name == "Plantillas")
            {
                <div class="col-sm-3">
                    <div class="level-1">
                        <div>

                        </div>
                        @foreach (var subMenuItems in menuMain.Children)
                        {
                            if (subMenuItems.MenuLevel == 2 && subMenuItems.Name == "Nueva Plantilla")
                            {
                                <div class="level-2">
                                    <div>
                                        <span class="subTitleMenu">@subMenuItems.Name</span>
                                    </div>
                                    @foreach (var subSubMenuItems in subMenuItems.Children)
                                    {
                                        if (subSubMenuItems.ParentId == 11)
                                        {
                                            <div class="level-3">
                                                <div class="row-cols-1">
                                                    <div class="form-group d-flex margin-0">
                                                        <input type="checkbox" id="@subSubMenuItems.Id" name="ValuesSelected" value="@menuMain.Id,@subMenuItems.Id,@subSubMenuItems.Id" disabled />
                                                        <label for="@subSubMenuItems.Id" class="label-check">@subSubMenuItems.Name</label>
                                                    </div>
                                                </div>
                                            </div>
                                        }

                                    }
                                </div>
                            }
                        }
                    </div>
                </div>
            }
            if (menuMain.MenuLevel == 1 && menuMain.Children.Count > 0 && menuMain.Name == "Plantillas")
            {
                <div class="col-sm-3">
                    <div class="level-1">
                        <div>

                        </div>
                        @foreach (var subMenuItems in menuMain.Children)
                        {
                            if (subMenuItems.MenuLevel == 2 && subMenuItems.Name == "Operaciones")
                            {
                                <div class="level-2">
                                    <div>
                                        <span class="subTitleMenu">@subMenuItems.Name</span>
                                    </div>
                                    @foreach (var subSubMenuItems in subMenuItems.Children)
                                    {
                                        if (subSubMenuItems.ParentId == 12)
                                        {
                                            <div class="level-3">
                                                <div class="row-cols-1">
                                                    <div class="form-group d-flex margin-0">
                                                        <input type="checkbox" id="@subSubMenuItems.Id" name="ValuesSelected" value="@menuMain.Id,@subMenuItems.Id,@subSubMenuItems.Id" disabled />
                                                        <label for="@subSubMenuItems.Id" class="label-check">@subSubMenuItems.Name</label>
                                                    </div>
                                                </div>
                                            </div>
                                        }

                                    }
                                </div>
                            }
                        }
                    </div>
                </div>
            }
        }
    </div>
</div>
